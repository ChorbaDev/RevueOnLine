import java.sql.*;
public class metier.Periodicite {
	private String libelle;
	private metier.Connexion maConnexion= new metier.Connexion();
	private Connection Connect=maConnexion.creeConnexion();
	
	public metier.Periodicite(String ch) {
		this.libelle=ch;
	}
	
	public void addPeriodicite(metier.Periodicite p) throws SQLException {
		String sql="insert into metier.Periodicite(libelle) values (?)";
		PreparedStatement requete=Connect.prepareStatement(sql);
		
	}
}
